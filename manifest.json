{
    "manifest_version": 3,
    "name": "Selector Code Generator",
    "version": "1.0",
    "description": "クリックした要素を取得するJSコードを生成します",
    "permissions": ["activeTab", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["content.css"]
      }
    ]
  }